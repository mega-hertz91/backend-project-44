{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "eslint-config-airbnb-base"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "import/extensions": [
            "error",
            {
                "js": "always"
            }
        ],
        "no-underscore-dangle": [
            2,
            {
                "allow": [
                    "__filename",
                    "__dirname"
                ]
            }
        ]
    },
    "plugins": [

    ],
    "ignorePatterns": [
        "./.github/**/**.*"
    ]
}
